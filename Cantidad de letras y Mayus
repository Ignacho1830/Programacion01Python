print("Este programa cuenta la cantidad de letras")
palabra = input("Ingrese la frase o palabra: ")

for i in palabra:  
    if i == "a":
        i = "A"
        print (i, end="")
    elif i == "e":
        i = "E"
        print (i, end="")
        i == "i"
        i = "I"
        print (i, end="")
        i == "o"
        i = "O"
        print (i, end="")
        i == "u"
        i = "U"
        print(i, end="")
    else:
        print (i, end="")

        
